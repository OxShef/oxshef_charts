title = {{Lithium ion storage between graphenes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3211259{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
year = {2011}
}
