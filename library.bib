month = {apr},
number = {13},
pages = {5570--9},
pmid = {22385301},
title = {{Computer simulations of nucleation of nanoparticle superclusters from solution.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22385301},
volume = {28},
year = {2012}
}
