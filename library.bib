journal = {Journal of the American Chemical Society},
keywords = {Calcium Carbonate,Calcium Carbonate: chemistry,Calcium Chloride,Calcium Chloride: chemistry,Crystallization,Neutron Diffraction,Ovalbumin,Ovalbumin: chemistry,Protein Conformation,Protein Folding,Scattering, Small Angle,Water,Water: chemistry},
month = {may},
number = {21},
pages = {6879--92},
pmid = {18452291},
title = {{Nucleation and growth of CaCO3 mediated by the egg-white protein ovalbumin: a time-resolved in situ study using small-angle neutron scattering.}},
