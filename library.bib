month = {feb},
pages = {139--43},
pmid = {22265653},
publisher = {Elsevier B.V.},
title = {{Determination of perfluorooctane sulfonate and perfluorooctanoic acid in food packaging using liquid chromatography coupled with tandem mass spectrometry.}},
