file = {::},
issn = {1752-153X},
journal = {Chemistry Central journal},
month = {jan},
number = {1},
pages = {46},
pmid = {21801445},
publisher = {Chemistry Central Ltd},
title = {{Nanostructured AgBr loaded TiO2: An efficient sunlight active photocatalyst for degradation of Reactive Red 120.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3159096{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
year = {2011}
}
