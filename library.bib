@phdthesis{ObukohwoOgherodia2004,
abstract = {This project work considers natural fibre reinforced composites for wind turbine blades and nacelle cover manufacture. The King palm leaves which are readily available, were sourced to extract the staple fibres which were thereafter used as reinforcement for the epoxy and unsaturated polyester matrices. The composite fabrication was done using the hand lay-up method via the use of a silicon mould (for epoxy matrix) and a metallic mould (for UPR matrix). The composite samples obtained were thereafter, tested for the effect of the fibres on the matrices. A mechanical tensile test was precisely carried out on both samples of the epoxy and unsaturated polyester (UPR) composites. The results gotten were analysed to see the effect the King palm staple fibre has on the epoxy and UPR matrix. Also the effects of fibre content on some mechanical properties were also considered. From the result analysis, conclusions were drawn on how suitable natural King palm staple fibres could serve as good reinforcement in polymer composites.},
author = {{Obukohwo Ogherodia}},
file = {::},
keywords = {Composites,Epoxy,King Palm natural Fibre,Tensile strength,Unsaturated Polyester (UPR)},
number = {November},
