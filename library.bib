file = {::},
issn = {0362-2436},
journal = {Spine},
keywords = {1633,1639,39,a,biomechanics,coital position,coitus,level of evidence,low back pain,lumbar spine,n,quality of life,sexual intercourse,spine 2014},
month = {sep},
number = {20},
pages = {1633--1639},
title = {{Male Spine Motion During Coitus: implications for the low back pain patient.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25208042 http://content.wkhealth.com/linkback/openurl?sid=WKPTLP:landingpage{\&}an=00007632-201409150-00005},
volume = {39},
