journal = {Journal of biomedical materials research},
keywords = {Biocompatible Materials,Humans,Microscopy, Electron, Scanning,Phosphonic Acids,Platelet Adhesiveness,Surface Properties,Thrombosis,Thrombosis: etiology,Thrombosis: prevention {\&} control},
