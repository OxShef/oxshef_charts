month = {oct},
number = {4},
pages = {2322--34},
pmid = {15454433},
publisher = {Elsevier},
title = {{Rheology and ultrasound scattering from aggregated red cell suspensions in shear flow.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1304656{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {87},
year = {2004}
}
