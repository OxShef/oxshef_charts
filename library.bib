month = {nov},
number = {45},
pages = {17570--4},
pmid = {17978189},
title = {{Magnetic field-induced assembly of oriented superlattices from maghemite nanocubes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2077042{\&}tool=pmcentrez{\&}rendertype=abstract},
