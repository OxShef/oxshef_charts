@article{Ingrosso2010,
abstract = {The unique size- and shape-dependent electronic properties of nanocrystals (NCs) make them extremely attractive as novel structural building blocks for constructing a new generation of innovative materials and solid-state devices. Recent advances in material chemistry has allowed the synthesis of colloidal NCs with a wide range of compositions, with a precise control on size, shape and uniformity as well as specific surface chemistry. By incorporating such nanostructures in polymers, mesoscopic materials can be achieved and their properties engineered by choosing NCs differing in size and/or composition, properly tuning the interaction between NCs and surrounding environment. In this contribution, different approaches will be presented as effective opportunities for conveying colloidal NC properties to nanocomposite materials for micro and nanofabrication. Patterning of such nanocomposites either by conventional lithographic techniques and emerging patterning tools, such as ink jet printing and nanoimprint lithography, will be illustrated, pointing out their technological impact on developing new optoelectronic and sensing devices.},
