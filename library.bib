month = {jun},
number = {1},
pages = {97--107},
pmid = {12767710},
title = {{A general approach to describe the antimicrobial agent release from highly swellable films intended for food packaging applications.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12767710},
volume = {90},
year = {2003}
}
