file = {:Users/martinjohnhadley/Google Drive/AcademicPapers/MendeleyTidy/1999/1999{\_}Gibson, Best, Bonfield{\_}Chemical characterization of silicon-substituted hydroxyapatite.pdf:pdf},
issn = {0021-9304},
journal = {Journal of biomedical materials research},
keywords = {Biocompatible Materials,Biocompatible Materials: chemistry,Calcium,Calcium: analysis,Ceramics,Ceramics: chemistry,Chemical Precipitation,Chemistry,Crystallization,Durapatite,Durapatite: chemistry,Fourier Transform Infrared,Hot Temperature,Materials Testing,Phosphorus,Phosphorus: analysis,Physical,Physicochemical Phenomena,Powders,Silicon,Silicon: chemistry,Spectroscopy,X-Ray Diffraction},
month = {mar},
number = {4},
pages = {422--8},
pmid = {10397946},
title = {{Chemical characterization of silicon-substituted hydroxyapatite.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10397946},
