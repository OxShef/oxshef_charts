@article{Wang2009g,
abstract = {One-dimensional (1D) CdS@ZnS core-shell nanocomposites were successfully synthesized via a two-step solvothermal method. Preformed CdS nanowires with a diameter of ca. 45 nm and a length up to several tens of micrometers were coated with a layer of ZnS shell by the reaction of zinc acetate and thiourea at 180 °C for 10 h. It was found that uniform ZnS shell was composed of ZnS nanoparticles with a diameter of ca. 4 nm, which anchored on the nanowires without any surface pretreatment. The 1D CdS@ZnS core-shell nanocomposites were confirmed by XRD, SEM, TEM, HR-TEM, ED, and EDS techniques. The optical properties and photocatalytic activities of the 1D CdS@ZnS core-shell nanocomposites towards methylene blue (MB) and 4-chlorophenol (4CP) under visible light ($\lambda$ {\textgreater} 420 nm) were separately investigated. The results show that the ZnS shell can effectively passivate the surface electronic states of the CdS cores, which accounts for the enhanced photocatalytic activities of the 1D CdS@ZnS core-shell nanocomposites compared to that of the uncoated CdS nanowires.},
