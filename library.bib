publisher = {Springer Open Ltd},
title = {{Effect of the Nd content on the structural and photoluminescence properties of silicon-rich silicon dioxide thin films.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3211213{\&}tool=pmcentrez{\&}rendertype=abstract},
