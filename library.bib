journal = {Journal of biomedical materials research. Part A},
keywords = {Biocompatible Materials,Cell Adhesion,Cell Adhesion: drug effects,Durapatite,Durapatite: pharmacology,Electron,Fourier Transform Infrared,Mechanical,Microscopy,Nanostructures,Scanning,Silicon,Silicon: pharmacology,Spectrophotometry,Spectroscopy,Static Electricity,Stress},
month = {aug},
number = {2},
pages = {352--63},
pmid = {16646067},
title = {{Silicon addition to hydroxyapatite increases nanoscale electrostatic, van der Waals, and adhesive interactions.}},
