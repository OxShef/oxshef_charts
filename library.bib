title = {{Experimental and theoretical studies of nanofluid thermal conductivity enhancement: a review.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3211287{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
year = {2011}
}
