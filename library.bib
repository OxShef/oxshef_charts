file = {::},
issn = {1556-276X},
journal = {Nanoscale research letters},
month = {jan},
number = {1},
pages = {371},
pmid = {21711884},
publisher = {Springer Open Ltd},
title = {{Novel hybrid organic/inorganic 2D quasiperiodic PC: from diffraction pattern to vertical light extraction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3211462{\&}tool=pmcentrez{\&}rendertype=abstract},
