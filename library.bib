pages = {622--7},
pmid = {18835221},
title = {{Incidence of anterior cruciate ligament injury and other knee ligament injuries: a national population-based study.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18835221},
volume = {12},
