issn = {1422-0067},
journal = {International journal of molecular sciences},
keywords = {(Zn/Al, Mg/Al)-layered double hydroxides,RBL2H3 cell,cetirizine,controlled release,human Chang liver cells,nanocomposite},
month = {jan},
number = {5},
pages = {5899--916},
pmid = {22754339},
title = {{In vitro inhibition of histamine release behavior of cetirizine intercalated into zn/al- and mg/al-layered double hydroxides.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3382767{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {13},
year = {2012}
}
