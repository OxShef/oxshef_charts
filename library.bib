keywords = {Animals,Biocompatible Materials,Biocompatible Materials: chemistry,Biocompatible Materials: diagnostic use,Biocompatible Materials: therapeutic use,Biocompatible Materials: toxicity,Ferric Compounds,Ferric Compounds: chemistry,Humans,Magnetics,Nanomedicine,Nanomedicine: standards,Nanoparticles,Nanoparticles: chemistry,Nanoparticles: diagnostic use,Nanoparticles: therapeutic use,Nanoparticles: toxicity,Tissue Distribution},
month = {jun},
number = {6},
pages = {467--77},
pmid = {19389434},
publisher = {Elsevier B.V.},
title = {{Magnetic nanoparticles for theragnostics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2700776{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {61},
