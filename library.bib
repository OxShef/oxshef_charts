title = {{Shuttle-like supramolecular nanostructures formed by self-assembly of a porphyrin via an oil/water system.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3212066{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {6},
