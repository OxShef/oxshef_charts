pmid = {14528050},
title = {{Role of protein aggregation in mitochondrial dysfunction and neurodegeneration in Alzheimer's and Parkinson's diseases.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/14528050},
volume = {4},
year = {2003}
}
