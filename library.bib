pages = {7683--7},
pmid = {1502184},
title = {{Magnetite biomineralization in the human brain.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=49775{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {89},
year = {1992}
